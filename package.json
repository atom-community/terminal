{
  "name": "terminal",
  "title": "Atom Terminal",
  "main": "./dist/terminal",
  "repository": "https://github.com/atom-ide-community/terminal",
  "bugs": {
    "url": "https://github.com/atom-ide-community/terminal/issues/new/choose"
  },
  "version": "0.0.0",
  "description": "The xterm based terminal for Atom",
  "keywords": [
    "terminal",
    "xterm",
    "term",
    "console",
    "shell",
    "emulator",
    "pty",
    "tty",
    "comspec",
    "command-line",
    "bash",
    "sh",
    "powershell",
    "cmd"
  ],
  "activationHooks": [
    "core:loaded-shell-environment"
  ],
  "atomTestRunner": "./spec/custom-runner",
  "license": "MIT",
  "engines": {
    "atom": ">=1.50.0 <2.0.0"
  },
  "providedServices": {
    "platformioIDETerminal": {
      "description": "Run commands and open terminals.",
      "versions": {
        "1.1.0": "providePlatformIOIDEService"
      }
    },
    "terminal": {
      "description": "Change the terminal.",
      "versions": {
        "1.0.0": "provideTerminalService"
      }
    }
  },
  "consumedServices": {
    "tool-bar": {
      "versions": {
        "^0 || ^1": "consumeToolBar"
      }
    }
  },
  "dependencies": {
    "fs-extra": "^9.1.0",
    "node-pty-prebuilt-multiarch": "^0.9.0",
    "uuid": "^8.3.2",
    "whatwg-url": "^8.4.0",
    "which": "^2.0.2",
    "xterm": "4.10.0",
    "xterm-addon-fit": "0.5.0",
    "xterm-addon-ligatures": "^0.4.0",
    "xterm-addon-web-links": "0.4.0",
    "xterm-addon-webgl": "0.9.0"
  },
  "devDependencies": {
    "@types/atom": "1.40.7",
    "@types/fs-extra": "^9.0.6",
    "@types/jasmine": "^3.6.3",
    "@types/node": "^14.14.22",
    "@types/resize-observer-browser": "^0.1.5",
    "@types/uuid": "^8.3.0",
    "@types/whatwg-url": "^8.0.0",
    "@types/which": "^2.0.0",
    "atom-jasmine3-test-runner": "^5.1.8",
    "build-commit": "0.1.4",
    "cross-env": "7.0.3",
    "eslint": "^7.19.0",
    "eslint-config-atomic": "^1.6.1",
    "npm-check-updates": "11.1.1",
    "prettier": "^2.2.1",
    "rollup": "2.38.4",
    "rollup-plugin-atomic": "^2.0.1",
    "shx": "0.3.3",
    "temp": "^0.9.4",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint .",
    "test": "atom --test spec",
    "clean": "shx rm -rf dist",
    "tsc": "tsc -p src/tsconfig.json",
    "dev:tsc": "tsc --watch -p src/tsconfig.json",
    "dev": "npm run clean && cross-env NODE_ENV=development cross-env BABEL_KEEP_MODULES=true rollup -c -w",
    "build": "npm run clean && cross-env NODE_ENV=production cross-env BABEL_KEEP_MODULES=true rollup -c",
    "build-commit": "build-commit -o dist",
    "bump": "ncu -u -x coffeescript",
    "prepare": "npm run build"
  },
  "deserializers": {
    "TerminalModel": "deserializeTerminalModel"
  }
}
